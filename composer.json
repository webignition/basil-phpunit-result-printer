{
    "name": "webignition/basil-phpunit-result-printer",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/webignition/basil-phpunit-result-printer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\BasilPhpUnitResultPrinter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\BasilPhpUnitResultPrinter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs-ecs": "./vendor/bin/ecs check src tests --ansi --config ecs.yaml",
        "cs-phpcs": "./vendor/bin/phpcs",
        "cs": [
            "@cs-ecs",
            "@cs-phpcs"
        ],
        "static-analysis": "./vendor/bin/phpstan analyse --ansi",
        "test": "./vendor/bin/phpunit --colors=always",
        "ci": [
            "@composer cs",
            "@composer static-analysis",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.4",
        "phpunit/phpunit": "~9",
        "symfony/yaml": "^5.1",
        "webignition/base-basil-test-case": ">=0.37,<1",
        "webignition/basil-dom-identifier-factory": ">=0.13,<1",
        "webignition/basil-identifier-analyser": ">=0.7,<1",
        "webignition/basil-models": ">=0.56,<1",
        "webignition/basil-runner-documents": ">=0.1,<1",
        "webignition/basil-value-type-identifier": ">=0.1,<1",
        "webignition/php-dom-element-identifier": ">=0.14,<1",
        "webignition/symfony-panther-dom-crawler-navigator": ">=0.19,<1",
        "webignition/yaml-document-generator": ">=0.2,<1"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-mockery": "^0.12.0",
        "squizlabs/php_codesniffer": "3.*",
        "symplify/easy-coding-standard": "^8.1",
        "webignition/basil-parser": ">=0.25,<1",
        "webignition/object-reflector": ">=0.2,<1",
        "webignition/yaml-document-set-parser": "^0.1.0"
    },
    "config": {
        "sort-packages": true
    }
}
